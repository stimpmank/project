<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

    <url-mapping id="login">
        <pattern value="/login" />
        <view-id value="/secure/login.pf" />	
    </url-mapping>

    <url-mapping id="principal">
        <pattern value="/principal" />
        <view-id value="/secure/pages/principal.pf" />	
    </url-mapping>
        
    <url-mapping id="programacion">
        <pattern value="/programacion" />
        <view-id value="/secure/pages/programacion.pf" />	
    </url-mapping>
    
    <url-mapping id="gestionHorarios">
        <pattern value="/gestionHorarios" />
        <view-id value="/secure/pages/gestionHorarios/gestionHorarios.pf" />	
    </url-mapping>
    
    <url-mapping id="detalleNovedades">
        <pattern value="/detalleNovedades" />
        <view-id value="/secure/pages/detalleNovedades.pf" />	
    </url-mapping>
    
    <url-mapping id="agenda">
        <pattern value="/agenda" />
        <view-id value="/secure/pages/agenda.pf" />	
    </url-mapping>
    
    <url-mapping id="error">
        <pattern value="/error" />
        <view-id value="/secure/error.pf" />	
    </url-mapping>
        
    <url-mapping id="accesoden">
        <pattern value="/accesoden" />
        <view-id value="/secure/accesoDenegado.pf" />	
    </url-mapping>

</pretty-config>